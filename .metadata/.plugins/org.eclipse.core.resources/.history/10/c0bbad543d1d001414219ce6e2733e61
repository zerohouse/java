import junit.framework.TestCase;


public class SadariTest extends TestCase {
	
	Sadari sadari;

	protected void setUp() throws Exception {
		super.setUp();
		sadari = new Sadari(5,3);
		sadari.addLine(2, 2);
		sadari.addLine(3, 3);
	}

	protected void tearDown() throws Exception {
		super.tearDown();
	}



}


