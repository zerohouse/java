import junit.framework.TestCase;


public class CalculratorTest extends TestCase {

	public void testAdd() {
		
		Calculrator cal = new Calculrator();
		int res = cal.add(2,1);
		assertEquals(3,res);

	}
	
	public void testSub() throws Exception {
		Calculrator cal = new Calculrator();
		int res = cal.sub(2,1);
		assertEquals(1,res);
		
	}

}
