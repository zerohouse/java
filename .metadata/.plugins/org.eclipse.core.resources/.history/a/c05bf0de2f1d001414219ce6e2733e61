import java.util.ArrayList;

public class Sadari {
	ArrayList<Point> points;
	Point marker;

	Sadari() {
		points = new ArrayList<Point>();
	}

	public void moveMarker() {
		for (Point point : points) {
			if (point.row == marker.row && point.column == marker.column) {

				marker.row += point.row;
				marker.column += point.column;
			}
		}

	}

}
