package strcal;

public class strcal {
	
	int add(String x){
		int res=0;
		String[] tmp = x.split(",|\n");
		for (String s:tmp){
			try{
			res+=Integer.parseInt(s);}
			catch(Exception e){			
			}
		}
		return res;
	}
	
	public static void main(String[] args) {
		strcal x = new strcal();
		System.out.println(x.add("1,\n5"));
		
	}

}
