import junit.framework.TestCase;


public class CalculratorTest extends TestCase {

	Calculrator cal;
	
	@Override
	protected void setUp() throws Exception {
		
		cal = new Calculrator();
	}
	
	
	public void testAdd() {
		
		
		int res = cal.add(2,1);
		assertEquals(3,res);

	}
	
	public void testSub() throws Exception {
		
		int res = cal.sub(2,1);
		assertEquals(1,res);
		
	}

}
